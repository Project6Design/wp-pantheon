{
    "name": "project6/wordpress",
    "description": "Custom Wordpress site",
    "repositories":[
        {
            "type":"composer",
            "url":"https://wpackagist.org"
        }
    ],
    "require": {
        "wpackagist-plugin/wordpress-seo":">=7.0.2",
        "wpackagist-plugin/kint-debugger": "^1.2",
        "wpackagist-plugin/manual-image-crop": "^1.12",
        "wpackagist-plugin/redirection": "^3.6",
        "wpackagist-plugin/regenerate-thumbnails": "^3.0",
        "wpackagist-plugin/timber-library": "^1.8",
        "wpackagist-plugin/user-role-editor": "^4.47",
        "wpackagist-plugin/wp-native-php-sessions": "^0.6.9",
        "wpackagist-plugin/wp-redis": "^0.7.0"
    },
    "autoload": {
        "psr-4": {
            "Project6\\": "src/Project6",
            "Sobrato\\": "src/Sobrato"
        }
    }
}
